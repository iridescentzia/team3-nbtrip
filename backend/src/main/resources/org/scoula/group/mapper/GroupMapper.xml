<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "--//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.scoula.group.mapper.GroupMapper">

    <select id="getGroupsById" resultType="org.scoula.group.domain.GroupVO">
        select * from trip where trip_id=#{id}
    </select>
    <select id="getGroupsByOwnerId" resultType="org.scoula.group.domain.GroupVO">
        select * from trip where owner_id=#{id}
    </select>
</mapper>